{
  "configurations": [
    {
      "browse": {
        "databaseFilename": "",
        "limitSymbolsToIncludedHeaders": true
      },
      "name": "Win32",
      "includePath": ["${workspaceFolder}/**"],
      "defines": ["_DEBUG", "UNICODE", "_UNICODE"],
      "compilerPath": "gcc.exe",
      "cStandard": "c17",
      "cppStandard": "c++11",
      "intelliSenseMode": "gcc-x64"
    },
    {
      "name": "Linux",
      "includePath": ["${workspaceFolder}/**"],
      "defines": ["DEBUG", "UNICODE", "_UNICODE"],
      "compilerPath": "/usr/bin/gcc",
      "cStandard": "c17",
      "cppStandard": "c++11",
      "intelliSenseMode": "linux-gcc-x64"
    },
    {
      "name": "Mac",
      "includePath": ["${workspaceFolder}/**"],
      "defines": ["_DEBUG", "UNICODE", "_UNICODE"],
      "macFrameworkPath": [
        "/System/Library/Frameworks",
        "/Library/Frameworks",
        "${workspaceFolder}/**"
      ],
      "compilerPath": "/usr/local/bin/gcc-13",
      "cStandard": "c17",
      "cppStandard": "c++11",
      "intelliSenseMode": "macos-gcc-x64"
    }
  ],
  "version": 4
}
